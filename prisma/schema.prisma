generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @default(uuid(7))
  username String @unique
  name     String

  createdAt DateTime @default(now()) @map("created_at")

  @@map("users")
}
